{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/main/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "React-Flow",
  "id": "fci-reactflow-app",
  "backend": true,
  "executable": "gpx_react_flow",
  "info": {
    "keywords": ["app"],
    "description": "",
    "author": {
      "name": "Fci"
    },
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Tag management",
      "path": "/a/%PLUGIN_ID%/tag-management",

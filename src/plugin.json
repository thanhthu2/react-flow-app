    {
      "type": "page",
      "name": "Relation management",
      "path": "/a/%PLUGIN_ID%/relation-management",
      "role": "Admin",
      "addToNav": true,
      "defaultNav": false
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=10.4.0",
    "plugins": []
